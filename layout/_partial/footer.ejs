<div class="footer">
    <% if(path != 'index.html') { %>
        <span>© 2022-1999 nino 
            / <a href="/contact">联系</a>
            / <a href="/support">支持</a>
            / <a href="/subscribe">订阅</a>
            / <a href="/friends">近邻</a>
        </span>
    <% } %>
</div>
<div class="footer-last">
    <% if(path == 'index.html'){ %>
         <% if(theme.index.footer != null || theme.index.footer == "") { %>
            <span class="index-footer-last-span"><%= theme.index.footer %></span>
        <% } %>
    <% } else { %>
        <span class="footer-last-span"><%= theme.post.footer %></span>
    <% } %>
</div>

